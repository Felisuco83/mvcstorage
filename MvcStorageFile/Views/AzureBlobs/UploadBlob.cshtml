<h1>Upload Blob</h1>

<form method="post" enctype="multipart/form-data">
    <input type="hidden" name="containername" 
           value="@ViewData["CONTAINERNAME"]" />
    <input type="file" name="blobfile"
           class="form-control"/>
    <button type="submit" class="btn btn-outline-info">
        Upload
    </button>
</form>